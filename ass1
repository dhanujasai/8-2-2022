#include<stdio.h>
#include<conio.h>
int main()
{
int a[10],i,n;
printf("enter array size");
scanf("%d",&n);
printf("enter array elements");
for(i=0;i<n;i++)
scanf("%d",&a[i]);
for(i=0;i<n;i++)
{
if(a[i]%2==0)
{
printf("%d",a[i]);
}
}
printf("\n");
for(i=0;i<n;i++)
{
if(a[i]%2!=0)
{
printf("%d",a[i]);
}
}
return 0;
}
